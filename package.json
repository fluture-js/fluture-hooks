{
  "name": "fluture-hooks",
  "version": "2.1.3",
  "description": "Fantasy Land Monad and Alternative instances for Fluture's hook",
  "keywords": [
    "fluture",
    "monad",
    "fantasy-land",
    "resource-management"
  ],
  "type": "module",
  "main": "./dist/umd.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/umd.js"
    },
    "./index.js": "./index.js"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "doctest": "sanctuary-doctest",
    "lint": "sanctuary-lint",
    "release": "sanctuary-release",
    "test": "npm run lint && sanctuary-test && npm run doctest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fluture-js/fluture-hooks.git"
  },
  "files": [
    "/dist",
    "/index.js",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "author": "Aldwin Vlasblom <aldwin@avaq.it> (https://github.com/Avaq)",
  "license": "MIT",
  "dependencies": {
    "callgebra": "^2.0.0"
  },
  "peerDependencies": {
    "fluture": ">=12.0.0-beta.4 <15.0.0"
  },
  "devDependencies": {
    "c8": "^7.1.0",
    "codecov": "^3.2.0",
    "fluture": "^14.0.0",
    "oletus": "^3.0.0",
    "rollup": "^2.0.0",
    "sanctuary-scripts": "^4.0.0",
    "sanctuary-type-identifiers": "^3.0.0"
  }
}
